            </sch1:User>
         </sch:HeaderRequest>
         <sch:CustId>
            <!--Optional:-->
            <sch1:SPName></sch1:SPName>
            <!--Optional:-->
            <sch1:custPermId>${headers.custPermId}</sch1:custPermId>
            <!--Optional:-->
            <sch1:custLoginId></sch1:custLoginId>
            <!--Optional:-->
            <sch1:custType>${headers.custType}</sch1:custType>
