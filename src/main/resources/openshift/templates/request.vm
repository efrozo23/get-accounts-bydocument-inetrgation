                  <sch1:bankId>string</sch1:bankId>
                  <!--Optional:-->
                  <sch1:trnType>string</sch1:trnType>
               </sch1:MessageInfo>
               <!--Optional:-->
               <sch1:TrnInfoList>
                  <!--Zero or more repetitions:-->
                  <sch1:TrnInfo>
                     <!--Optional:-->
                     <sch1:trnCode>string</sch1:trnCode>
                     <!--Optional:-->
                     <sch1:trnSrc>string</sch1:trnSrc>
                  </sch1:TrnInfo>
               </sch1:TrnInfoList>
            </sch1:MessageHeader>
            <!--Optional:-->
            <sch1:User>
               <!--Optional:-->
               <sch1:userName>TEST</sch1:userName>
