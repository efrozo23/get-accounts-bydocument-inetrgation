      <sch:getAccountsDetailByDocumentRequest>
         <sch:HeaderRequest>
            <!--Optional:-->
            <sch1:MessageHeader>
               <!--Optional:-->
               <sch1:MessageKey>
                  <!--Optional:-->
                  <sch1:integrationId>string</sch1:integrationId>
                  <!--Optional:-->
                  <sch1:requestVersion>string</sch1:requestVersion>
                  <sch1:requestUUID>${headers.requestUUID}</sch1:requestUUID>
               </sch1:MessageKey>
               <!--Optional:-->
               <sch1:MessageInfo>
                  <sch1:dateTime>${headers.dateTime}</sch1:dateTime>
                  <sch1:systemId>string</sch1:systemId>
                  <!--Optional:-->
                  <sch1:originatorName>${headers.originatorName}</sch1:originatorName>
